{
	"version": 3,
	"routes": [
		{
			"src": "/404",
			"headers": {
				"Location": "/404/"
			},
			"status": 308
		},
		{
			"src": "/aboutus",
			"headers": {
				"Location": "/aboutus/"
			},
			"status": 308
		},
		{
			"src": "/en/aboutus",
			"headers": {
				"Location": "/en/aboutus/"
			},
			"status": 308
		},
		{
			"src": "/en/projects/([^/]+?)",
			"headers": {
				"Location": "/en/projects/$1/"
			},
			"status": 308
		},
		{
			"src": "/en/projects",
			"headers": {
				"Location": "/en/projects/"
			},
			"status": 308
		},
		{
			"src": "/en/services",
			"headers": {
				"Location": "/en/services/"
			},
			"status": 308
		},
		{
			"src": "/en",
			"headers": {
				"Location": "/en/"
			},
			"status": 308
		},
		{
			"src": "/projects/([^/]+?)",
			"headers": {
				"Location": "/projects/$1/"
			},
			"status": 308
		},
		{
			"src": "/projects",
			"headers": {
				"Location": "/projects/"
			},
			"status": 308
		},
		{
			"src": "/services",
			"headers": {
				"Location": "/services/"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/aboutus\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/aboutus\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/projects\\/([^/]+?)\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/projects\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/services\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/en\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/([^/]+?)\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/projects\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/services\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}
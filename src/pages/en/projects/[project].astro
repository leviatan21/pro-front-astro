---
// @ts-nocheck
import Page from '../../_project/index.astro'

import PortfoliosContent from '../../_projects/content/portfolios.json'

// https://docs.astro.build/en/reference/api-reference/#getstaticpaths
// https://docs.astro.build/en/guides/routing/#dynamic-routes
export function getStaticPaths() {
  return PortfoliosContent.map(project => {
    const path = project.path.replace('/projects/', '')
    return ({'params':{'project':path}})
  })
}

const {
  project=""
} = Astro.params
---
<Page project={project} />
